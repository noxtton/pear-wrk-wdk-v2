{
  "name": "@tetherto/pear-wrk-wdk",
  "version": "0.1.8",
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "rimraf *.d.ts src/**/*.d.ts; tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "gen:bundle": "npx bare-pack --target ios-arm64 --target ios-arm64-simulator --target ios-x64-simulator --target android-arm --target android-arm64 --target android-ia32 --target android-x64 --linked --defer ledger-bitcoin --imports ./pack.imports.json --out bundle/worklet.bundle.mjs src/worklet.mjs",
    "gen:schema": "node src/schema/schema.js && node src/lib/hrpc-docs-builder.js",
    "postinstall": "npm run gen:bundle"
  },
  "dependencies": {
    "@wdk/bare-ethers": "https://github.com/noxtton/wdk-bare-ethers-internal#a4027a713f1226f5b5faa191f5a73580e247abf4",
    "@tetherto/wdk-wallet": "1.0.0-beta.4",
    "@tetherto/wdk-wallet-btc": "1.0.0-beta.3",
    "@tetherto/wdk-wallet-evm": "1.0.0-beta.3",
    "@tetherto/wdk-wallet-evm-erc-4337": "1.0.0-beta.2",
    "@tetherto/wdk-wallet-solana": "1.0.0-beta.2",
    "@tetherto/wdk-wallet-ton": "1.0.0-beta.3",
    "@tetherto/wdk-wallet-ton-gasless": "1.0.0-beta.2",
    "@tetherto/wdk-wallet-tron": "1.0.0-beta.2",
    "@tetherto/wdk-wallet-tron-gasfree": "1.0.0-beta.2",
    "bare-ipc": "1.1.1",
    "encoding": "npm:bare-encoding@1.0.0",
    "hrpc": "4.0.1",
    "hyperschema": "1.12.4",
    "os": "npm:bare-os@3.6.1"
  },
  "devDependencies": {
    "rimraf": "6.0.1",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "files": [
    "bundle/worklet.bundle.d.mts"
  ],
  "author": "Tether",
  "license": "Apache-2.0"
}
