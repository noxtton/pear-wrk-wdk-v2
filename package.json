{
  "name": "@wdk/bare",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build:types": "rimraf *.d.ts **/*.d.ts; tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "gen:macos-bundle": "npx bare-pack --target darwin-arm64 --linked --out bundle/wdk-worklet.bundle.js src/wdk-worklet.js",
    "gen:mobile-bundle": "npx bare-pack --target ios-arm64 --target ios-arm64-simulator --target ios-x64-simulator --target android-arm --target android-arm64 --target android-ia32 --target android-x64 --linked --out bundle/wdk-worklet.mobile.bundle.js src/wdk-worklet.js",
    "gen:schema": "node src/schema/schema.js && node src/lib/hrpc-docs-builder.js",
    "postinstall": "npm run gen:mobile-bundle"
  },
  "dependencies": {
    "@wdk/wallet": "https://github.com/tetherto/wdk-wallet#develop",
    "@wdk/wallet-btc": "https://github.com/tetherto/wdk-wallet-btc#develop",
    "@wdk/wallet-evm": "https://github.com/tetherto/wdk-wallet-evm#develop",
    "@wdk/wallet-evm-erc-4337": "https://github.com/tetherto/wdk-wallet-evm-erc-4337#develop",
    "@wdk/wallet-solana": "https://github.com/tetherto/wdk-wallet-solana#develop",
    "@wdk/wallet-ton": "https://github.com/tetherto/wdk-wallet-ton#develop",
    "@wdk/wallet-ton-gasless": "https://github.com/tetherto/wdk-wallet-ton-gasless#develop",
    "@wdk/wallet-tron": "https://github.com/tetherto/wdk-wallet-tron#develop",
    "@wdk/wallet-tron-gasfree": "https://github.com/tetherto/wdk-wallet-tron-gasfree#develop",
    "bare-ipc": "^1.1.1",
    "bare-wdk-runtime": "^1.5.3",
    "bip39": "3.1.0",
    "encoding": "npm:bare-encoding@^1.0.0",
    "hrpc": "^4.0.1",
    "hyperschema": "^1.12.4",
    "os": "npm:bare-os@^3.6.1"
  },
  "devDependencies": {
    "rimraf": "6.0.1",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "author": "Tether",
  "license": "Apache-2.0"
}
