{
  "name": "@wdk/bare",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build:types": "rimraf *.d.ts **/*.d.ts; tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "gen:macos-bundle": "npx bare-pack --target darwin-arm64 --linked --out bundle/wdk-worklet.bundle.js src/wdk-worklet.js",
    "gen:mobile-bundle": "npx bare-pack --target ios-arm64 --target ios-arm64-simulator --target ios-x64-simulator --target android-arm --target android-arm64 --target android-ia32 --target android-x64 --linked --imports ./pack.imports.json --out bundle/wdk-worklet.mobile.bundle.js src/wdk-worklet.js",
    "gen:schema": "node src/schema/schema.js && node src/lib/hrpc-docs-builder.js",
    "postinstall": "npm run gen:mobile-bundle"
  },
  "dependencies": {
    "@wdk/wallet": "https://github.com/tetherto/wdk-wallet#bf07000b63945080343d5b9ae0968d202595d8e0",
    "@wdk/wallet-btc": "https://github.com/tetherto/wdk-wallet-btc#52cecd9f0c3ff409515fa58de7912da747bce4a3",
    "@wdk/wallet-evm": "https://github.com/tetherto/wdk-wallet-evm#bf6164ff683962799b9b548459b9a15ad004d04d",
    "@wdk/wallet-evm-erc-4337": "https://github.com/tetherto/wdk-wallet-evm-erc-4337#eb0f8299cd36ebfe28b6841cfb91b33c29039079",
    "@wdk/wallet-solana": "https://github.com/tetherto/wdk-wallet-solana#11b220b69f0d31a987c56a9c3ccfff07e75f5ae8",
    "@wdk/wallet-ton": "https://github.com/tetherto/wdk-wallet-ton#baadf63b0698312667259b588bcedc743b582601",
    "@wdk/wallet-ton-gasless": "https://github.com/tetherto/wdk-wallet-ton-gasless#b40f90f7251d5b4e7f6f2d9399804c2e9aaab49f",
    "@wdk/wallet-tron": "https://github.com/tetherto/wdk-wallet-tron#d0b2223bc840557a5243d92b419a1cb853fb70d6",
    "@wdk/wallet-tron-gasfree": "https://github.com/tetherto/wdk-wallet-tron-gasfree#c72291a9f513888189d94b81272d95ae68dade88",
    "bare-ipc": "1.1.1",
    "bare-wdk-runtime": "1.5.3",
    "bip39": "3.1.0",
    "encoding": "npm:bare-encoding@1.0.0",
    "hrpc": "4.0.1",
    "hyperschema": "1.12.4",
    "os": "npm:bare-os@3.6.1"
  },
  "devDependencies": {
    "rimraf": "6.0.1",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "author": "Tether",
  "license": "Apache-2.0"
}
